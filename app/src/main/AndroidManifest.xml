<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.miniproject.dlsestimator"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:fullBackupContent="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.miniproject.dlsestimator.MenuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoToolbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.miniproject.dlsestimator.CalculatorActivity"
            android:parentActivityName="com.miniproject.dlsestimator.MenuActivity">
        </activity>
        <activity
            android:name="com.miniproject.dlsestimator.AboutActivity"
            android:label="@string/about_label"
            android:parentActivityName="com.miniproject.dlsestimator.MenuActivity">
        </activity>
        <activity
            android:name="com.miniproject.dlsestimator.FaqActivity"
            android:label="@string/faq_button"
            android:parentActivityName="com.miniproject.dlsestimator.AboutActivity">
        </activity>
    </application>

</manifest>
